<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html lang="en-US" xmlns="http://www.w3.org/1999/xhtml" dir="ltr">

<head profile="http://gmpg.org/xfn/11">
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no" />

	<title>{{ landingPage.header|striptags }}</title>

	<link rel="stylesheet" href="/wp-content/plugins/zerotolaunch/themes/premium/photography/style.css" type="text/css" media="screen" />
	<link rel="shortcut icon" href="/wp-content/plugins/zerotolaunch/themes/premium/photography/images/favicon.ico" />
	<script type='text/javascript' src='/wp-includes/js/jquery/jquery.js?ver=1.11.0'></script>
	<script type='text/javascript' src='/wp-includes/js/jquery/jquery-migrate.min.js?ver=1.2.1'></script>
	<script type='text/javascript' src='/wp-content/plugins/zerotolaunch/themes/premium/photography/js/responsiveImages.js?ver=3.9'></script>
	<script type='text/javascript' src='/wp-content/plugins/zerotolaunch/themes/premium/photography/js/functions.js?ver=3.9'></script>

    <script type="text/javascript">
        if (typeof jQuery === 'undefined'){
            document.write("<scr" + "ipt type=\"text/javascript\" src=\"http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js\"></scr" + "ipt>");
        }
    </script>

	<link rel='canonical' href='/l/{{ landingPage.slug }}/' />

</head>
<body class="page page-id-75 page-template page-template-template-landing-php landing-page">

<div class="wrapper">
	<div class="header">
		<div class="shell cf">
			<h1 class="logo">
                <img {{ editing ? 'data-toggle="editable" id="ZTLContentLogo"' }} src="{{ landingPage.logo_url }}" alt="" style="max-height: 50px;">
            </h1><!-- /.logo notext -->
		</div><!-- /.shell -->
	</div><!-- /.header -->


	<div class="main">
		<div class="shell cf">
            <div class="headline" {{ editing ? 'contenteditable="true" data-toggle="editableContent"  id="ZTLContentHeader"' }}>
                {{ landingPage.header|raw }}
            </div>

			<div class="sidebar right">
                <div id="ZTLOptinForm">
                    {{ optinForm ? render_optin_form(optinForm, landingPage.id)|raw  }}
                </div>
			</div><!-- /.sidebar -->
            <div class="content left" {{ editing ? 'contenteditable="true" data-toggle="editableContent" id="ZTLContentBody"' }}>
                {{ landingPage.body|raw }}
            </div><!-- /.content -->

		</div><!-- /.shell -->
	</div><!-- /.main -->

	<div class="footer-push notext">&nbsp;</div><!-- /.footer-push notext -->
</div><!-- /.wrapper -->
<div class="footer">
	<div class="shell cf">

        <div class="copyright" {{ editing ? 'contenteditable="true" data-toggle="editableContent" id="ZTLContentFooter"' }}>&copy Copyright 2014 Obscura Photography. All rights reserved.</div>
	</div><!-- /.nav -->
</div><!-- /.footer -->

{% if editing %}
    {% include 'editor_code.php' %}
{% else %}
    <script type="text/javascript">
        jQuery(document).ready(function(){
            jQuery.get("?logView={{ optinForm.slug }}&id={{ optinForm.id }}&type=landing_page&landingPageID={{ landingPage.id }}&c=" + new Date().toDateString());
            // Fix for Vertical Optin Widgets
            jQuery('#ZTLOptinForm form').addClass('widget');
        });
    </script>
{% endif %}

</body>
</html>